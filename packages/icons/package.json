{
  "name": "@kaizen/icons",
  "version": "0.0.0",
  "description": "A collection of icons",
  "scripts": {
    "prepublish": "tsc --project tsconfig.dist.json",
    "build": "yarn clean && yarn prepublish",
    "build:watch": "yarn clean && yarn prepublish --watch",
    "clean": "rimraf '**/*.d.ts' '**/*.js' '**/*.map'",
    "icons:generate": "npx ts-node build/script.ts",
    "icons": "npx @svgr/cli -d build/icons-compiled build/icons-raw/academy.icon.svg --template build/template.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cultureamp/kaizen-design-system.git",
    "directory": "packages/icons"
  },
  "bugs": {
    "url": "https://github.com/cultureamp/kaizen-design-system/issues"
  },
  "files": [
    "**/*",
    "!**/*.ts",
    "!**/*.tsx",
    "**/*.d.ts",
    "!**/*.spec.*",
    "!**/*.snap",
    "!draft-templates",
    "!stories",
    "!docs",
    "!tsconfig.dist.json"
  ],
  "author": "",
  "private": false,
  "license": "MIT",
  "devDependencies": {
    "@svgr/cli": "^5.5.0",
    "@svgr/core": "6.0.0-alpha.0",
    "@svgr/plugin-jsx": "^5.5.0",
    "glob": "^7.2.0",
    "lodash.camelcase": "^4.3.0",
    "rimraf": "^3.0.2"
  },
  "peerDependencies": {
    "react": "^16.9.0"
  },
  "dependencies": {
    "@svgr/plugin-prettier": "^5.5.0",
    "@svgr/plugin-svgo": "^5.5.0",
    "lodash.startcase": "^4.4.0"
  }
}
